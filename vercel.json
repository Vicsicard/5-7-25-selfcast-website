{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "https://5-7-25-selfcast-cms.vercel.app/api/$1"
    },
    {
      "src": "/cms",
      "dest": "https://5-7-25-selfcast-cms.vercel.app"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "trailingSlash": false,
  "env": {
    "NODE_ENV": "production",
    "DEBUG": "true"
  }
}
